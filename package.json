{
  "name": "vl-ui-footer",
  "version": "0.0.0",
  "description": "De Vlaanderen footer.",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "test:grid": "echo TODO",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-footer.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-footer.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-footer/style.css' vl-footer.js",
    "release:prepare:commit": "git add -f vl-footer.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server -p 9000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-footer.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-footer/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-footer#readme",
  "dependencies": {
    "vl-ui-core": "^2.0.0"
  },
  "peerDependencies": {
    "vl-ui-core": "^2.0.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-util": "^3.9.1",
    "vl-ui-util": "^3.2.1"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
